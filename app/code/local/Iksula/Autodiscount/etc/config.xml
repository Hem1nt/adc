<?xml version="1.0"?>
<config>
  <modules>
    <Iksula_Autodiscount>
      <version>0.1.0</version>
    </Iksula_Autodiscount>
  </modules>
  <global>
    <helpers>
      <autodiscount>
        <class>Iksula_Autodiscount_Helper</class>
      </autodiscount>
    </helpers>
    <models>
      <autodiscount>
        <class>Iksula_Autodiscount_Model</class>
        <resourceModel>registercode_mysql4</resourceModel>
    </autodiscount>
    </models>
    <events>
      <checkout_cart_product_add_after>
          <observers>
            <checkout_cart_product_add_afterhandler_observer>
              <type>singleton</type>
              <class>autodiscount/observer</class>
              <method>autoCustomerDiscount</method>
            </checkout_cart_product_add_afterhandler_observer>
          </observers>
        </checkout_cart_product_add_after>
    </events>
  </global>
</config> 