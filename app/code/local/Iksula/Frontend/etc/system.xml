              <comment></comment>
              <sort_order>1</sort_order>
              <show_in_default>1</show_in_default>
              <show_in_website>1</show_in_website>
              <show_in_store>1</show_in_store>
            </selected_status>
            <order_date translate="label comment" module="frontend">
              <label>Order Date For Pay Payment</label>
              <frontend_type>text</frontend_type>
              <comment></comment>
              <sort_order>1</sort_order>
              <show_in_default>1</show_in_default>
              <show_in_website>1</show_in_website>
              <show_in_store>1</show_in_store>
            </order_date>
          </fields>
        </general>
      </groups>
    </order_status>
